<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>simplewebapp</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>ServletHora</display-name>
    <servlet-name>ServletHora</servlet-name>
    <servlet-class>curso.jboss.servlet.ServletHora</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletHora</servlet-name>
    <url-pattern>/ServletHora</url-pattern>
  </servlet-mapping>
  <session-config>
 	<session-timeout>30</session-timeout>
 </session-config>
</web-app>